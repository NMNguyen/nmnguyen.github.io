               {\n                    name: \"Phuong\",\n                    email: \"phuong@cjs.vn\",\n                }\n            ];\n        }\n    }\n});\n",{"version":3,"sources":["Users.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiKA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Users.vue","sourceRoot":"src/components/settings","sourcesContent":["<template>\n    <div class=\"settings\">\n        <div class=\"main-content\">\n            <!-- ngIf: activeTab == \"plan\" -->\n            <!-- ngIf: activeTab == \"planBilling\" -->\n            <!-- ngIf: activeTab == \"planConfirm\" -->\n            <!-- ngIf: activeTab == \"planConfirmComplete\" -->\n            <!-- ngIf: activeTab == \"users\" -->\n            <div class=\"team\">\n                <div class=\"header\">\n                    <h1>Quản lý các thành viên</h1>\n                    <p>Quản lý nhóm người tham gia vào nỗ lực tuyển dụng tại Cloudjet.</p>\n                    <div class=\"position\">\n                        <div class=\"section invite\">\n                            <button class=\"show-add-member btn btn-blue\" @click=\"show()\">\n                                <i class=\"fa fa-envelope\"></i>\n                                <span>Mời thành viên</span>\n                            </button>\n                        </div>\n                        <div class=\"section\">\n                            <h2>Pending Invitations</h2>\n                            <ul class=\"list member-list\">\n                                <!-- ngRepeat: invite in team_invited -->\n                            </ul>\n                        </div>\n                        <div class=\"section\">\n                            <h2>Thành viên</h2>\n                            <b-list-group>\n                                <b-list-group-item class=\"d-flex align-items-center\" v-for=\"user in members\" :key=\"user.id\">\n                                    <div class=\"\" style=\"width: 64px\">\n                                        <img :src=\"user.avatar || 'https://ui-avatars.com/api/?name=' + user.name\" alt=\"\"\n                                             class=\"avatar rounded-circle\">\n                                    </div>\n                                    <div class=\"flex-grow-1 p-2\">\n                                        <h4><strong>{{user.name}}</strong></h4>\n                                        <small>{{user.email}}</small>\n                                    </div>\n                                    <div class=\"p-2\">\n                                        <i class=\"fas fa-users\"></i>&nbsp;\n                                        Team\n                                    </div>\n                                    <div class=\"p-2\">\n                                        <b-dropdown variant=\"light\" text=\"Company Admin\">\n                                            <b-dropdown-item>Company Admin</b-dropdown-item>\n                                            <b-dropdown-item>Member</b-dropdown-item>\n                                        </b-dropdown>\n                                    </div>\n                                    <div class=\"p-2\">\n                                        <a href=\"#\" @click=\"console.log('delete')\">\n                                            <i class=\"fas fa-times\"></i>\n                                        </a>\n                                    </div>\n                                </b-list-group-item>\n                            </b-list-group>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <!-- end ngIf: activeTab == \"users\" -->\n            <!-- ngIf: activeTab == \"teams\" -->\n            <!-- ngIf: activeTab == \"billing\" -->\n            <!-- ngIf: activeTab == \"general\" -->\n        </div>\n        <modal name=\"invite-members\" :width=\"500\" :height=\"420\">\n            <div class=\"modal-content\">\n                <div class=\"close\" @click=\"hide()\">\n                    <i class=\"fa fa-times\"></i>\n                </div>\n                <div class=\"modal-body\">\n                    <div class=\"team-permissions add-membership\">\n                        <div class=\"header\">\n                            <h1>Invite Users</h1>\n                            <p>\n                                Invite new users by email address.\n\n                            </p>\n                            <div class=\"section invite manage-hiring-teams\">\n                                <form name=\"form.formInviteUser\" novalidate=\"novalidate\">\n                                    <div class=\"section invitation-container\">\n                                        <h2>Invite by Email</h2>\n                                        <input\n                                            class=\"invite\"\n                                            type=\"text\"\n                                            placeholder=\"e.g. kyle@company.com, julia@company.com\"\n                                            required=\"required\"\n                                        >\n                                        <input\n                                            type=\"password\"\n                                            class=\"invite\"\n                                            placeholder=\"password\"\n                                            required=\"required\"\n                                        >\n                                        <ul class=\"user-type-selector\" id=\"rad\">\n                                            <li class=\"user-type-option\">\n                                                <label for=\"radio-admin\">\n                                                    <input\n                                                        v-model=\"x\"\n                                                        type=\"radio\"\n                                                        name=\"user-type\"\n                                                        id=\"radio-admin\"\n                                                        value=\"admin\"\n                                                        required=\"required\"\n                                                    >\n                                                    <span>Add as Administrator</span>\n                                                </label>\n                                                <p v-show=\"x === 'admin'\">\n                                                    Company\n                                                    administrators can invite new users, create, manage &amp; view\n                                                    positions\n                                                    &amp; pools as well as email templates, questionnaires &amp; other\n                                                    company\n                                                    wide resources.\n                                                </p>\n                                            </li>\n                                            <li class=\"user-type-option\">\n                                                <label for=\"radio-member\">\n                                                    <input\n                                                        v-model=\"x\"\n                                                        type=\"radio\"\n                                                        name=\"user-type\"\n                                                        id=\"radio-member\"\n                                                        value=\"member\"\n                                                        required=\"required\"\n                                                    >\n                                                    <span>Add as Member</span>\n                                                </label>\n                                                <p v-show=\"x === 'member'\">\n                                                    Members\n                                                    only see the positions &amp; pools they're involved with.\n                                                </p>\n                                            </li>\n                                        </ul>\n                                    </div>\n                                    <div class=\"add-member-actions\">\n                                        <button class=\"btn btn-none\" type=\"button\">\n                                            <span>Cancel</span>\n                                        </button>\n                                        <button class=\"btn btn-blue\" type=\"submit\">\n                      <span>\n                        <i class=\"fa fa-envelope\"></i>\n                        <span class=\"form-inactive\">\n                          Invite\n                          User(s)\n                        </span>\n                        <span class=\"form-active\">Sending...</span>\n                      </span>\n                                        </button>\n                                    </div>\n                                </form>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </modal>\n    </div>\n</template>\n\n\n<script lang=\"ts\">\n    import Vue from \"vue\";\n    import {mapGetters} from \"vuex\";\n    import Company from \"@/models/Company\";\n    import {User} from \"firebase\";\n\n    export default Vue.extend({\n        data(){\n            return {\n                x:'member'\n            }\n        },\n        methods: {\n            show() {\n                this.$modal.show(\"invite-members\");\n            },\n            hide() {\n                this.$modal.hide(\"invite-members\");\n            },\n            // mix the getters into computed with object spread operator\n            ...mapGetters([\n                'currentUser',\n                'currentCompany',\n                // ...\n            ]),\n        },\n        computed: {\n            members(): any[]\n            {\n                return [\n                    {\n                        name: \"Hong\",\n                        email: \"hong@cjs.vn\",\n                    },\n                    {\n                        name: \"Phuong\",\n                        email: \"phuong@cjs.vn\",\n                    }\n                ];\n            }\n        }\n    });\n</script>\n"]}]}