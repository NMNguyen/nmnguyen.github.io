\">\n    import Vue from 'vue';\n\n    export default Vue.extend({\n        name: 'FilterColumn',\n        props: ['open'],\n        computed: {\n            classes(): any\n            {\n                return {\n                    \"flex-shrink-0\": true,\n                    \"d-flex\": true,\n                    \"flex-column\": true,\n                    \"filterNav\": true,\n                    \"closed\": !this.open,\n                }\n            }\n        }\n    })\n</script>\n"]}]}